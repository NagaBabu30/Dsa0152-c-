#include<iostream>
using namespace std;
int main()
{
	float a,b;
	cout<<"enter number 1 =";
	cin>>a;
	cout<<"enter number 2 =";
	cin>>b;
	cout<<"addition is ="<<a+b<<endl;
	cout<<"subtraction is ="<<a-b<<endl;
	cout<<"multiplication is ="<<a*b<<endl;
	if(a>b){
		cout<<"divison is ="<<a/b<<endl;
	}
	else{
		cout<<"division is not possible";
	}
}
